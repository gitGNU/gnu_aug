<package name="aug">

  <quotincl>augctx/config.h</quotincl>

  <interface name="mpool">

    <method name="malloc" type="void*" qname="malloc">
      <param name="size" type="size_t"/>
    </method>

    <method name="free" type="void" qname="free">
      <param name="ptr" type="void*"/>
    </method>

    <method name="realloc" type="void*" qname="realloc">
      <param name="ptr" type="void*"/>
      <param name="size" type="size_t"/>
    </method>

    <method name="calloc" type="void*" qname="calloc">
      <param name="nmemb" type="size_t"/>
      <param name="size" type="size_t"/>
    </method>

  </interface>

</package>
