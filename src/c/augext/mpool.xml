<package name="aug" dir="augext">

  <interface name="mpool">

    <method name="allocmem" type="void*" qname="allocmem">
      <param name="size" type="size_t"/>
    </method>

    <method name="freemem" type="void" qname="freemem">
      <param name="ptr" type="void*"/>
    </method>

    <method name="reallocmem" type="void*" qname="reallocmem">
      <param name="ptr" type="void*"/>
      <param name="size" type="size_t"/>
    </method>

    <method name="callocmem" type="void*" qname="callocmem">
      <param name="nmemb" type="size_t"/>
      <param name="size" type="size_t"/>
    </method>

  </interface>

</package>
